{
  "name": "@dsz-examaware/core",
  "version": "1.0.2",
  "description": "ExamAware Core",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "prepublishOnly": "pnpm run build",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "examaware",
    "exam",
    "config",
    "parser",
    "validation"
  ],
  "author": "hello8693 <hello8693@hello8693.xyz>",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3"
  }
}
